{
  "viewportWidth": 600,
  "viewportHeight": 800,
  "ignoreTestFiles": [
    "**/answer/*",
    "*answer.js",
    "*.d.ts",
    "**/__snapshots__/*",
    "**/__image_snapshots__/*",
    "**/13-app-actions/todo-page-object.js",
    "**/13-app-actions/utils.js",
    "**/17-component-testing/*.*"
  ],
  "baseUrl": "http://localhost:3000",
  "env": {
    "cypress-plugin-snapshots": {
      "autoCleanUp": true,
      "autopassNewSnapshots": false,
      "diffLines": 3
    }
  },
  "component": {
    "testFiles": "./cypress/integration/17-component-testing/*.comp-test.{js,ts,jsx,tsx}",
    "componentFolder": "./cypress/integration/17-component-testing"
  },
  "retries": {
    "runMode": 3,
    "openMode": 1
  },
  "$schema": "https://on.cypress.io/cypress.schema.json",
  "experimentalStudio": true
}
